t;\r\n}\r\n\r\nbody{\r\n  background: $bg-gradient;\r\n  color: $white;\r\n}\r\n\r\n@import \"./components/navbar\";\r\n@import \"./components/product\";\r\n@import \"./components/bannerHeader\";\r\n@import \"./components/basket\";","src\\styles\\components\\_navbar.scss":".navbar{\r\n  @extend .center-h;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 100;\r\n  background-color: #131921;\r\n  padding: .5rem 1rem;\r\n  justify-content: space-between;\r\n\r\n  .logo{\r\n    cursor: pointer;\r\n    font-weight: 900;\r\n    letter-spacing: 1px;\r\n    font-size: 1.5rem;\r\n  }\r\n\r\n  .searchbar{\r\n    @extend .center-h;\r\n    margin: 0 1rem;\r\n    flex: 1;\r\n    height: 30px;\r\n    border-radius: .25rem;\r\n    overflow: hidden;\r\n\r\n    .searchInput{\r\n      height: 100%;\r\n      width: 90%;\r\n      border: none;\r\n      padding: 0 .3rem;\r\n      outline: none;\r\n    }\r\n    .searchBtn{\r\n      @extend .center;\r\n      cursor: pointer;\r\n      height: 100%;\r\n      width: 10%;\r\n      background-color: darken(#fff, 10%);\r\n    }\r\n  }\r\n\r\n  .options{\r\n    @extend .center-h;\r\n    padding: .5rem 0;\r\n\r\n    .option{\r\n      cursor: pointer;\r\n      margin: 0 .1rem;\r\n      border-radius: .25rem;\r\n      padding: 0 .5rem;\r\n\r\n      &.welcome{\r\n        cursor: unset;\r\n      }\r\n    }\r\n\r\n    .categoriesBtn{\r\n      @extend .center-h;\r\n    }\r\n    \r\n    .welcome{\r\n      // flex: 1;\r\n    }\r\n\r\n    .basket{\r\n      position: relative;\r\n      @extend .center-h;\r\n\r\n      .basketCount{\r\n        margin-left: .3rem;\r\n        font-weight: bold;\r\n      }\r\n    }\r\n  }\r\n}","src\\styles\\components\\_product.scss":".products{\r\n  width: 85%;\r\n  margin: 0 auto;\r\n  margin-top: 3rem;\r\n  margin-bottom: 1rem;\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-gap: 2rem;\r\n\r\n  .product{\r\n    position: relative;\r\n    overflow: hidden;\r\n    background-color: $white;\r\n    color: $black;\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 1.5rem;\r\n    border-radius: .15rem;\r\n\r\n    &:hover{\r\n      .addToCartBtn{\r\n        transform: translateY(0);\r\n      }\r\n    }\r\n\r\n    .product_titlePriceRow{\r\n      display: flex;\r\n      justify-content: space-between;\r\n    }\r\n    .product_title{\r\n      cursor: pointer;\r\n      font-size: 1.3rem;\r\n\r\n      &:hover{\r\n        text-decoration: underline;\r\n      }\r\n    }\r\n    .product_price{\r\n      font-weight: 500;\r\n    }\r\n    .product_image{\r\n      margin: 1rem 0;\r\n    }\r\n    .product_image{\r\n      img{\r\n        width: 100%;\r\n        height: 200px;\r\n        object-fit: contain;\r\n      }\r\n    }\r\n    .addToCartBtn{\r\n      @extend .center;\r\n      cursor: pointer;\r\n      position: absolute;\r\n      bottom: 0;\r\n      left: 0;\r\n      width: 100%;\r\n      height: fit-content;\r\n      padding: .3rem 0;\r\n      background: linear-gradient(135deg, #000000dd, #000000aa);\r\n      color: $white;\r\n      transition: .3s;\r\n      transform: translateY(100%);\r\n    }\r\n  }\r\n\r\n}","src\\styles\\components\\_bannerHeader.scss":".bannerHeader{\r\n  width: 100%;\r\n  height: 300px;\r\n\r\n  .bannerImg{\r\n    width: 100%;\r\n    height: 100%;\r\n    mask-image: linear-gradient(to bottom, rgba(0, 0, 0, 1), rgba(0, 0, 0, 1), rgba(0, 0, 0, 1), rgba(0, 0, 0, 0));\r\n    -webkit-mask-image: linear-gradient(to bottom, rgba(0, 0, 0, 1), rgba(0, 0, 0, 1), rgba(0, 0, 0, 1), rgba(0, 0, 0, 0));\r\n  }\r\n}","src\\styles\\components\\_basket.scss":".imagePreview_background{\r\n  z-index: 101;\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  @extend .center;\r\n  background-color: rgba($black, .7);\r\n\r\n  .imagePreview_box{\r\n    width: fit-content;\r\n    height: fit-content;\r\n    max-width: 40%;\r\n    max-height: 95%;\r\n\r\n    img{\r\n      width: 100%;\r\n      height: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n.basketContainer{\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  margin-top: 3rem;\r\n\r\n  .basketTotal_row{\r\n    @extend .center-h;\r\n    justify-content: space-between;\r\n    margin-bottom: 1rem;\r\n  }\r\n\r\n}\r\n\r\n.basketItem{\r\n  position: relative;\r\n  display: flex;\r\n  background-color: $white;\r\n  color: $black;\r\n  margin-bottom: .5rem;\r\n  padding: 2rem;\r\n\r\n  .img{\r\n    cursor: pointer;\r\n    img{\r\n      height: 200px;\r\n      object-fit: contain;\r\n    }\r\n  }\r\n\r\n  .infos{\r\n    margin-left: 3rem;\r\n\r\n    .titlePrice_row{\r\n      justify-content: space-between;\r\n    }\r\n\r\n    .description{\r\n      margin-top: 1rem;\r\n      font-size: .8rem;\r\n    }\r\n\r\n    .rating{\r\n      position: absolute;\r\n      bottom: 3rem;\r\n    }\r\n  }\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"8278c741c63554e2e38cc0fd314a1678","cacheData":{"env":{}}}